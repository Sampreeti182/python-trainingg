class Calculator:
    def add(self,a,b):
        return a+b
    def sub(self,a,b):
        return a-b
    def mul(self,a,b):
        return a*b
    def div(self,a,b):
        try:
            return a/b
        except ZeroDivisionError:
            print("Division by zero")
calc= Calculator()
print(calc.add(1,2))
print(calc.sub(3,4))
print(calc.mul(3,4))
print(calc.div(3,4))
